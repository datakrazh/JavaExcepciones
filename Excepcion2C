public class Excepcion2C {

    public static void main(String[] args) {
        System.out.println("***Ejemplo de excepci√≥n 2C***");
        try {
            System.out.println("Estoy en un bloque try ahora");
        }
        finally {
            System.out.println("Estoy en el inicio del bloque finally ahora");
            int a=5;
            int b=0;
            int c=a/b;
            System.out.println("c="+c);
            System.out.println("Estoy en en final del bloque finally ahora");
        }
    }
    
}
