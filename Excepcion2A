public class Excepcion2A {

    public static void main(String[] args) {
        System.out.println("***Ejemplo de excepción 2A***");
        try {
            int a=5;
            int b=0;
            int c=a/b;
            System.out.println("Estoy en la línea 9 ahora");
            System.out.println("c="+c);
        }
        catch(ArrayIndexOutOfBoundsException ex) {
        System.out.println("Atrapó a ArrayIndexOutBoundsException: "+ex.getMessage());
        ex.printStackTrace();    
        }
        
    }
    
}
